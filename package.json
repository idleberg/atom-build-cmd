{
  "name": "build-cmd",
  "version": "0.3.0",
  "description": "Atom Build provider for the Windows Command Prompt, runs Batch scripts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idleberg/atom-build-cmd.git"
  },
  "license": "MIT",
  "keywords": [
    "batch",
    "windows",
    "cmd",
    "bat"
  ],
  "main": "lib/provider.js",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "builder": {
      "description": "Runs Batch scripts",
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "package-deps": [
    "build"
  ],
  "dependencies": {
    "atom-package-deps": "^4.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "coffeelint-stylish": "^0.1.2",
    "eslint": "^3.8.1",
    "eslint-config-atom-build": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-jshint": "^2.0.2",
    "gulp-jsonlint": "^1.1.2",
    "gulp-lesshint": "^2.0.0",
    "jshint": "^2.9.4"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
